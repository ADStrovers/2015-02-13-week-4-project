<div>
  <form action="/edit/<%= params[:type] %>">
    <% view_output(@obj).each do |item| %>
      <%= item %>
    <% end %>
    <% if session[:username] == Person.get_username(@obj.creator_id) %>
      <p><button type="submit">Click Here to Edit This <%= params[:type].capitalize %></button></p>
    <% end %>
  </form>
  <form action="/attend/<%= params[:type] %>">
    <input hidden="true" name="id" value="<%= @obj.id %>">
    <p>
      <button type="attend">Click Here to Attend</button>
    </p>
  </form>
  <% unless @panels == [] || @panels == nil %>
    <p>Panels Registered For This Convention:</p>
    <% @panels.each do |x| %>
      <a href="/view/panel?id=<%= x.id %>"><%= x.name %></a>
    <% end %>
  <% end %>
</div>